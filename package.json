{
  "name": "divly-api",
  "version": "1.0.0",
  "description": "Divly Api Service",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --exec ts-node -e ts --project tsconfig.json src/server.ts",
    "studio": "prisma studio",
    "schema-format": "prisma format",
    "migrate": "prisma migrate dev",
    "migrate-reset": "prisma migrate reset",
    "webhook-test": "webhookthing",
    "test": "ts-mocha -r esm --paths --timeout 60000 -p ./tsconfig.json src/**/*.spec.ts --exit",
    "token:host": "ts-node --project tsconfig.json src/utils/testutils/gencustomertesttoken.ts",
    "token:customer": "ts-node --project tsconfig.json src/utils/testutils/genhosttesttoken.ts",
    "bot:jwt": "ts-node --project tsconfig.json src/utils/createBotToken.ts",
    "clear-db": "ts-node --project tsconfig.json src/utils/clearDb.ts",
    "test-push-notification": "ts-node --project tsconfig.json src/tests/testExpoNotifications.ts",
    "invite": "ts-node --project tsconfig.json src/utils/inviteAdmin.ts",
    "mtn-test-user": "ts-node --project tsconfig.json src/utils/mtn/scripts/generate.ts",
    "mtn-test-user-tokens": "ts-node --project tsconfig.json src/utils/mtn/scripts/get-access-token.ts",
    "generate-app-secret": "ts-node --project tsconfig.json src/utils/generateAppSecret.ts",
    "seed": "ts-node --project tsconfig.json src/utils/seedData.ts",
    "create-adopters": "ts-node --project tsconfig.json src/utils/createAdopter.ts",
    "delete-user": "ts-node --project tsconfig.json src/utils/deleteUserAccount.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/niebexco/niebex-api.git"
  },
  "bugs": {
    "url": "https://github.com/niebexco/niebex-api/issues"
  },
  "homepage": "https://github.com/niebexco/niebex-api#readme",
  "dependencies": {
    "@prisma/client": "^5.4.2",
    "@sentry/node": "^7.53.1",
    "@types/cookie-parser": "^1.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dayjs": "^1.11.8",
    "dotenv": "^16.0.3",
    "envalid": "^7.3.1",
    "expo-server-sdk": "^3.7.0",
    "express": "^4.18.2",
    "express-validator": "^6.14.3",
    "firebase-admin": "^11.5.0",
    "got": "11.8.6",
    "http-errors": "^2.0.0",
    "lodash": "^4.17.21",
    "mongoose": "^6.9.0",
    "morgan": "^1.10.0",
    "postmark": "^3.0.15",
    "stripe": "^11.13.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/expect": "^24.3.0",
    "@types/express": "^4.17.16",
    "@types/http-errors": "^2.0.1",
    "@types/lodash": "^4.14.191",
    "@types/mocha": "^10.0.1",
    "@types/morgan": "^1.9.4",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.33.0",
    "esm": "^3.2.25",
    "jsonwebtoken": "^9.0.0",
    "mocha": "^10.2.0",
    "ngrok": "^4.3.3",
    "nodemon": "^2.0.20",
    "prisma": "^5.4.2",
    "supertest": "^6.3.3",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5",
    "webhookthing": "^0.6.1"
  }
}
